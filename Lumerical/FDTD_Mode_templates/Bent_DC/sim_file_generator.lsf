device = 'BentDC_';

select('::model');
w_bus = get('w_bus') * 1e6;
w_ring = get('w_ring') * 1e6;
r_ring = get('r_ring') * 1e6;
gap = get('gap') * 1e6;


parameters = 'WUpperWG' + num2str(w_bus) + '_WLowerWG' + num2str(w_ring) +  
'_' + 'gap' + num2str(gap) + '_' + 'radius' + num2str(r_ring);

file_name = device + parameters + '.fsp';
save(file_name);

